package com.constructor.org;
abstract class Hotel {
	public String hotelLocation;

	abstract void menu();
}
class Novotel extends Hotel {
	String hotelLocation;
	public Novotel(String string, String string2) {
		// TODO Auto-generated constructor stub
	}
	public void Novotel(String hotelLocation ,String mainLocation) {
		super.hotelLocation =hotelLocation;
		this.hotelLocation=mainLocation;
		
	}
	public String toString() {
		return "novotel [hotellocatin ="+hotelLocation+" ,getclass :"+getClass()+" hashcode : "+hashCode()
+" , tostring()= " +super.toString() + "}";	
}
	void menu() {
		System.out.println("1.dosa , 2.idly , 3.bonda");
	}
}
public class MainClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       Novotel hotel = new Novotel("hyderabad" ,"mumbai");
       System.out.println(hotel.toString());
       hotel.menu();
	}

}
