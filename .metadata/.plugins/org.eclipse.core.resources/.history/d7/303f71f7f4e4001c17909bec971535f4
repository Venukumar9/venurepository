package com.constructor.org;
abstract class Hotel {
	double startingcost;
	public  Hotel (double staringcost) {
		this.startingcost=staringcost;
	}
	abstract double buffaeCost (double priceforBuffae);
}
class Novotel extends Hotel {
	Novotel(){
		super(520.00);
	}
	public double buffaeCost(double priceforBuffae) {
		return this.startingcost;
		
	}
}

public class Hotels {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
           Hotel hotel = new Novotel();
           System.out.println(hotel.buffaeCost(6766.00));
           
           
	}

}
